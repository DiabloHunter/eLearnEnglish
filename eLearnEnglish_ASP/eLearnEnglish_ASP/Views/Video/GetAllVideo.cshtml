@{
    ViewData["Title"] = "All video";
}
@model IEnumerable<VideoModel>



<div class="container">
    <h3 class="display-4"> All video view</h3>
    <div class="row">

        @foreach (var video in Model)
        {
            <div class="col-md-4">
                <partial name="_videoThumbnail" model="@video" />

            </div>
        }


    </div>
</div>
